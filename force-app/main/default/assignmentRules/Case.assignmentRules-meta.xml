<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Construction Case Assignment</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>Construction_High_Prio_Case</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Case.Priority</field>
                <operation>equals</operation>
                <value>High</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Construction_Medium_Case</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Case.Priority</field>
                <operation>equals</operation>
                <value>Medium</value>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Construction_Low_Case</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Case.Priority</field>
                <operation>equals</operation>
                <value>Low</value>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
